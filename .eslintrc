{
   "extends":[
      "airbnb",
      "airbnb/hooks",      
      "prettier",
      "prettier/react",
      "plugin:prettier/recommended",
      "eslint-config-prettier"
   ],
   "parser":"babel-eslint",
   "rules":{
      "prettier/prettier":[
         "error",
         {
            "semi": true,
            "trailingComma":"all",
            "printWidth":120
         }
      ]
   },
   "plugins":[
      "prettier"
   ]
}